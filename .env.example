# Configurações do Servidor
HOST=0.0.0.0
PORT=8000
DEBUG=false

# Configurações do Banco de Dados
POSTGRES_USER=browser_use
POSTGRES_PASSWORD=your_secure_password
POSTGRES_DB=browser_use_db
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
DATABASE_URL=postgresql://${POSTGRES_USER}:${POSTGRES_PASSWORD}@${POSTGRES_HOST}:${POSTGRES_PORT}/${POSTGRES_DB}

# Configurações do Playwright
PLAYWRIGHT_BROWSERS_PATH=/tmp/playwright-browsers
BROWSER_USE_DEBUG=true
BROWSER_USE_HEADLESS=true

# Configurações de API
GOOGLE_API_KEY=your_google_api_key
OPENAI_API_KEY=your_openai_api_key
ANTHROPIC_API_KEY=your_anthropic_api_key

# Configurações de Segurança
SECRET_KEY=your_secret_key_here
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# Configurações de Cache
CACHE_TTL=300
MAX_CACHE_SIZE=1000

# Configurações de Sessão
SESSION_TIMEOUT=300
MAX_SESSIONS=5

# Configurações de Log
LOG_LEVEL=INFO
LOG_FILE=/var/log/browser-use/app.log

# Configurações de telemetry (opcional)
TELEMETRY_ENABLED=false

# Set to false to disable anonymized telemetry
ANONYMIZED_TELEMETRY=true

# LogLevel: Set to debug to enable verbose logging, set to result to get results only. Available: result | debug | info
BROWSER_USE_LOGGING_LEVEL=info

# Configurações do Sistema
TASK_TIMEOUT=300  # Tempo limite em segundos (5 minutos)
WEBHOOK_URL=https://vrautomatize-n8n.snrhk1.easypanel.host/webhook/browser-use-vra-handler
NOTIFY_RUN_URL=https://vrautomatize-n8n.snrhk1.easypanel.host/webhook/notify-run
STATUS_URL=https://vrautomatize-n8n.snrhk1.easypanel.host/webhook/status

# Configurações do Browser
PLAYWRIGHT_BROWSERS_PATH=/tmp/playwright-browsers
BROWSER_USE_HEADLESS=true
BROWSER_USE_DEBUG=false
